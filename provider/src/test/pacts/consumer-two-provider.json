{
    "provider": {
        "name": "provider"
    },
    "consumer": {
        "name": "consumer-two"
    },
    "interactions": [
        {
            "description": "get single movie",
            "request": {
                "method": "GET",
                "path": "/movies/b3fc0be8-463e-4875-9629-67921a1e00f4"
            },
            "response": {
                "status": 200,
                "headers": {
                    "Content-Type": "application/hal+json"
                },
                "body": {
                    "_links": {
                        "self": {
                            "href": "http://some-url/resource"
                        }
                    },
                    "metacriticScore": 0.79,
                    "releaseYear": 2008,
                    "title": "Iron Man"
                },
                "matchingRules": {
                    "$.body.title": {
                        "match": "type"
                    },
                    "$.body.releaseYear": {
                        "match": "number"
                    },
                    "$.body.metacriticScore": {
                        "match": "number"
                    },
                    "$.body._links.self.href": {
                        "match": "type"
                    }
                }
            },
            "providerState": "Getting movie with any ID returns Iron Man"
        }
    ],
    "metadata": {
        "pact-specification": {
            "version": "2.0.0"
        },
        "pact-jvm": {
            "version": "3.5.11"
        }
    }
}